<%= form_with url: "/register", method: :post do |f| %>
  <%= f.label "Name:" %>
  <%= f.text_field :name %>
  <%= f.label "Email:" %>
  <%= f.text_field :email %>
  <%= f.label "Password:" %>
  <%= f.password_field :password %>
  <%= f.label "Confirm Password:" %>
  <%= f.password_field :confirm_password %>
  <%= f.submit "Submit" %>
<% end %>