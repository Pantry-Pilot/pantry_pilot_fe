<%= form_with url: "/login", method: :post, html: { class: "my-form" } do |f| %>
  <%= f.label "Email:" %>
  <%= f.text_field :email %>
  <%= f.label "Password:" %>
  <%= f.password_field :password %>
  <%= f.submit "Login" %>
<% end %>

<%= button_to "Log In with Google", "/auth/google_oauth2", data: { turbo: false } %>
